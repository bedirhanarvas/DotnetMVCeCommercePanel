@model List<eCommercePanel.DAL.DTOs.UserDTOs.Responses.UserDto>

@{
    ViewData["Title"] = "UserList";
}


<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
    </tr>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
            </tr>
        }
        
    </tbody>
</table>
